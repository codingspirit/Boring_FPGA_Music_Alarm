PROCESS(CLK)

VARIABLE CLKDIV:INTEGER;

BEGIN
	IF (CLK'EVENT AND CLK='1') THEN 
			
	IF CLKDIV=78643 THEN
				
	CLKDIV:=0;
				
	QL<= NOT QL;
			
	ELSE CLKDIV:=CLKDIV+1;
			
	END IF;
        END IF;
END PROCESS;
TWO:PROCESS(QL,BUZZER3)

BEGIN 
	
	IF BUZZER3='1' THEN COUNT<=199;
		
	ELSIF (QL'EVENT AND QL='1') THEN
           		
	IF  COUNT=0 THEN
					   
	COUNT<=0;
				
	ELSE COUNT<=COUNT-1;
		       
